<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProyectoSGCE</title>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap-5.3.0/dist/css/bootstrap.css" /><!--carga el estilo-->
    <script src="~/lib/bootstrap-5.3.0/dist/js/bootstrap.bundle.min.js"></script><!--este abre el drop-->
    <script src="~/lib/bootstrap-5.3.0/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
